pipeline {
  agent any 
  stages {
    stage('checkout') { 
      steps {
        checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/ashokmahato/ML.git']]])
      }
    }
    stage('statuscheck') {
          steps {
              BufferedReader br = new BufferedReader(new FileReader(/Users/ashok/.jenkins/jobs/GVpipeline1/workspace/abc.csv));
while ((line = br.readLine()) != null) {
    // use comma as separator
    String[] cols = line.split(",");
    println(cols[4]);
}
          }
      }
      
    }
  }
